{
  "name": "@paciolan/remote-module-loader",
  "version": "1.0.0-symantec-versioning",
  "description": "",
  "main": "dist/main.js",
  "author": "Paciolan",
  "license": "",
  "repository": {
    "type": "ssh",
    "url": "git@gitlabdev.paciolan.info:development/library/library/remote-module-loader.git"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production",
    "build:dev": "NODE_ENV=development webpack --mode development",
    "clean": "rimraf dist",
    "cz": "git-cz",
    "test": "jest",
    "test:changed": "npm run test -- --changedSince HEAD",
    "test:coverage": "npm run test -- --coverage",
    "lint": "eslint ."
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-transform-regenerator": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "babel-loader": "^8.0.5",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "git-cz": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.3.0",
    "prettier": "^1.16.4",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm run build && npm run test:changed",
      "pre-push": "npm run test:coverage"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
